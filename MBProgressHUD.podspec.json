{
  "name": "MBProgressHUD",
  "version": "1.1.0",
  "summary": "An iOS activity indicator view.",
  "description": "MBProgressHUD is an iOS drop-in class that displays a translucent HUD \nwith an indicator and/or labels while work is being done in a background thread. \nThe HUD is meant as a replacement for the undocumented, private UIKit UIProgressHUD \nwith some additional features.",
  "homepage": "http://www.bukovinski.com",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "sy": "yang.song@govee.com"
  },
  "source": {
    "git": "https://github.com/sygovee/MBProgressHUD.git",
    "tag": "1.1.0"
  },
  "platforms": {
    "ios": "11.0"
  },
  "frameworks": [
    "CoreGraphics",
    "QuartzCore"
  ],
  "requires_arc": true,
  "static_framework": true,
  "vendored_frameworks": "MBProgressHUD.xcframework",
  "ios": {
    "public_header_files": "MBProgressHUD.xcframework/ios-arm64/*/Headers/*.h",
    "source_files": "MBProgressHUD.xcframework/ios-arm64/*/Headers/*.h"
  }
}
